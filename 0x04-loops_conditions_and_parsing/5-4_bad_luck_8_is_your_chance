#!/usr/bin/env bash
# This script displays "Best School" 10 times, with specific messages on the 4th and 8th iterations

count=0

while [ "$count" -lt 10 ]; do
    if [ "$count" -eq 3 ]; then
        echo "bad luck"
    elif [ "$count" -eq 7 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi

    ((count++))
done
